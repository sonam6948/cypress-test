{
  "name": "licence-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode development --outDir dist-dev",
    "build:qa": "vue-tsc --noEmit && vite build --mode qa --outDir dist-qa",
    "build:preprod": "vue-tsc --noEmit && vite build --mode preprod --outDir dist-preprod",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod --outDir dist-prod",
    "preview": "vite preview",
    "test:e2e:qa:browserstack": "browserstack-cypress run --sync--project ./cypress --env configFile=qa",
    "test:e2e:qa:edge": "npx cypress run --project ./cypress --browser edge --env configFile=qa",
    "test:e2e:qa:webkit": "npx cypress run --project ./cypress --browser webkit --env tags=\"@smoke\" configFile=qa -e jsonOutput=reports/cucumber-json/cucumber-report-webkit.json",
    "test:e2e:open": "npx cypress open --project ./cypress  --browser chrome --env configFile=qa",
    "reports": "node ./cypress/cucumber-html-reporter.js",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:unit": "jest src",
    "test:unit:coverage": "vue-cli-service test:unit --coverage=true --no-cache",
    "lokalise:download": "node scripts/lokalise.js"
  },
  "dependencies": {
    "@cypress/xpath": "^2.0.3",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "luxon": "^3.3.0",
    "oidc-client-ts": "^2.2.4",
    "pinia": "^2.0.34",
    "pinia-plugin-persistedstate": "^3.1.0",
    "typescript": "^5.0.4",
    "vue": "^3.2.47",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/webpack-preprocessor": "^6.0.1",
    "cucumber-junit-convert": "^1.1.0",   
    "@cypress/xpath": "^2.0.3",
    "@lokalise/node-api": "^9.8.0",
    "@pinia/testing": "^0.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^7.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.8.7",
    "@types/unzipper": "^0.10.6",
    "@types/vue": "^2.0.0",
    "@types/webpack-env": "^1.18.0",
    "@vitejs/plugin-vue": "4.3.4",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0-alpha.2",
    "@vue/vue3-jest": "^29.2.4",
    "babel-loader": "^9.1.3",
    "browserstack-cypress-cli": "^1.28.1",
    "chai-colors": "^1.0.1",
    "cypress": "^12.14.0",
    "cypress-cloud": "^1.9.6",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-real-events": "^1.8.1",
    "cypress-vite": "^1.4.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "multiple-cucumber-html-reporter": "^1.21.6",
    "node-fetch": "^3.3.1",
    "playwright-webkit": "1.34",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.11",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.0.0-next.1",
    "ts-loader": "^9.4.2",
    "unzipper": "^0.10.11",
    "vite": "^4.2.0",
    "vite-plugin-environment": "^1.1.3",
    "vue-loader": "^17.2.2",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.8.19",
    "webpack": "^5.89.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "support/step_definitions/**/*.{js,ts}",
    "messages": {
      "enabled": true,
      "output": "reports/cucumber-ndjson/cucumber-report.ndjson"
    },
    "json": {
      "enabled": true,
      "formatter": "cucumber-json-formatter",
      "output": "reports/cucumber-json/cucumber-report.json"
    },
    "filterSpecs": true,
    "omitFiltered": true
  }
}
